@{
    Layout = "_Index";

}

<!-- =============================================== -->
<!-- Content Wrapper. Contains page content -->
<div class="content-wrapper">
    <!-- Content Header (Page header) -->
    <section class="content-header">
        <h1>
            Home
            <small>Welcome to the Game Panel</small>
        </h1>
        <ol class="breadcrumb">
            <li><a href="#"><i class="fa fa-dashboard"></i> Home</a></li>
            <li class="active">Dashboard</li>
        </ol>
    </section>

    <!-- Main content -->
    <section class="content">

        <div class="alert alert-danger alert-dismissable">
            <button type="button" data-id="26799" class="close announcement_close" data-dismiss="alert" aria-hidden="true">×</button>
            <h4><i class="fa fa-bell"></i> Having trouble connecting?</h4>
            <p>If you have trouble connecting to the server, you will need to reinstall your client via GTA:Network&#039;s announcement thread. You can uninstall the mod via the uninstaller in the GTA:N directory.</p>
        </div>


        <div class="row">
            <div class="col-md-6">
                <div class="box box-success">
                    <div class="box-header with-border">
                        <i class="fa fa-home"></i> <h3 class="box-title">News</h3>
                    </div>

                    <div class="box-body">
                        <p>Welcome to The Godfather's game panel!</p>

                        <div class="col-md-12">
                            <!-- The time line -->
                            <ul class="timeline">
                                <!-- timeline time label -->
                                <li class="time-label">
                                    <span class="bg-red">
                                        23 January 2017
                                    </span>
                                </li>
                                <!-- /.timeline-label -->
                                <!-- timeline item -->
                                <li>
                                    <i class="fa fa-envelope bg-blue"></i>

                                    <div class="timeline-item">
                                        <span class="time"><i class="fa fa-clock-o"></i> 12:05</span>

                                        <h3 class="timeline-header"><a href="#">Server Team</a> welcomes you!</h3>

                                        <div class="timeline-body">
                                            Welcome to The Godfather's game panel!
                                        </div>
                                        <div class="timeline-footer">
                                            <a class="btn btn-primary btn-xs">Read more</a>
                                            <a class="btn btn-danger btn-xs">Delete</a>
                                        </div>
                                    </div>
                                </li>
                            </ul>
                        </div>
                    </div>
                </div>
            </div>

            <div class="col-md-6">
                <div class="box box-success">
                    <div class="box-header with-border">
                        <i class="fa fa-home"></i> <h3 class="box-title">Server Status</h3>
                    </div>
                    <div class="box-body">
                        <p>You can view the server statistics here.</p>

                        <h4>Server</h4>
                        <div class="callout callout-success">
                            <h4><i class="fa fa-check"></i> The server is up and running!</h4>
                            <p>Yay!!</p>
                        </div>
                    </div>
                </div>
            </div>

        </div>
    </section>
    <!-- /.content -->
</div>
<!-- /.content-wrapper -->
    